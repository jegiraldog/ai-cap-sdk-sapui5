<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
    <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
        <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
    </edmx:Reference>
    <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
        <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
    </edmx:Reference>
    <edmx:DataServices>
        <Schema Namespace="AIService" xmlns="http://docs.oasis-open.org/odata/ns/edm">
            <Annotation Term="Core.Links">
                <Collection>
                    <Record>
                        <PropertyValue Property="rel" String="author"/>
                        <PropertyValue Property="href" String="https://cap.cloud.sap"/>
                    </Record>
                </Collection>
            </Annotation>
            <EntityContainer Name="EntityContainer">
                <ActionImport Name="getAvailableModels" Action="AIService.getAvailableModels"/>
                <ActionImport Name="askQuestion" Action="AIService.askQuestion"/>
                <ActionImport Name="healthCheck" Action="AIService.healthCheck"/>
                <ActionImport Name="uploadPDF" Action="AIService.uploadPDF"/>
                <ActionImport Name="processPDF" Action="AIService.processPDF"/>
                <ActionImport Name="askAboutPDF" Action="AIService.askAboutPDF"/>
                <ActionImport Name="listUploadedPDFs" Action="AIService.listUploadedPDFs"/>
            </EntityContainer>
            <ComplexType Name="return_AIService_getAvailableModels">
                <Property Name="id" Type="Edm.String"/>
                <Property Name="name" Type="Edm.String"/>
                <Property Name="modelName" Type="Edm.String"/>
                <Property Name="modelVersion" Type="Edm.String"/>
                <Property Name="status" Type="Edm.String"/>
                <Property Name="deploymentId" Type="Edm.String"/>
                <Property Name="configName" Type="Edm.String"/>
                <Property Name="scenarioId" Type="Edm.String"/>
                <Property Name="createdAt" Type="Edm.String"/>
                <Property Name="url" Type="Edm.String"/>
            </ComplexType>
            <ComplexType Name="return_AIService_askQuestion">
                <Property Name="question" Type="Edm.String"/>
                <Property Name="answer" Type="Edm.String"/>
                <Property Name="model" Type="Edm.String"/>
                <Property Name="deploymentId" Type="Edm.String"/>
                <Property Name="model_params" Type="AIService.return_AIService_askQuestion_model_params"/>
                <Property Name="timestamp" Type="Edm.String"/>
            </ComplexType>
            <ComplexType Name="return_AIService_askQuestion_model_params">
                <Property Name="temperature" Type="Edm.Double"/>
                <Property Name="max_tokens" Type="Edm.Int32"/>
                <Property Name="top_p" Type="Edm.Double"/>
            </ComplexType>
            <ComplexType Name="return_AIService_healthCheck">
                <Property Name="status" Type="Edm.String"/>
                <Property Name="service" Type="Edm.String"/>
                <Property Name="timestamp" Type="Edm.String"/>
            </ComplexType>
            <ComplexType Name="return_AIService_uploadPDF">
                <Property Name="success" Type="Edm.Boolean"/>
                <Property Name="fileName" Type="Edm.String"/>
                <Property Name="message" Type="Edm.String"/>
                <Property Name="timestamp" Type="Edm.String"/>
            </ComplexType>
            <ComplexType Name="return_AIService_processPDF">
                <Property Name="fileName" Type="Edm.String"/>
                <Property Name="extractedText" Type="Edm.String"/>
                <Property Name="wordCount" Type="Edm.Int32"/>
                <Property Name="summary" Type="Edm.String"/>
                <Property Name="timestamp" Type="Edm.String"/>
            </ComplexType>
            <ComplexType Name="return_AIService_askAboutPDF">
                <Property Name="question" Type="Edm.String"/>
                <Property Name="answer" Type="Edm.String"/>
                <Property Name="sourceDocument" Type="Edm.String"/>
                <Property Name="model" Type="Edm.String"/>
                <Property Name="deploymentId" Type="Edm.String"/>
                <Property Name="timestamp" Type="Edm.String"/>
            </ComplexType>
            <ComplexType Name="return_AIService_listUploadedPDFs">
                <Property Name="fileName" Type="Edm.String"/>
                <Property Name="uploadDate" Type="Edm.String"/>
                <Property Name="status" Type="Edm.String"/>
                <Property Name="wordCount" Type="Edm.Int32"/>
            </ComplexType>
            <Action Name="getAvailableModels" IsBound="false">
                <ReturnType Type="Collection(AIService.return_AIService_getAvailableModels)" Nullable="true"/>
            </Action>
            <Action Name="askQuestion" IsBound="false">
                <Parameter Name="question" Type="Edm.String" MaxLength="1000"/>
                <Parameter Name="selectedModel" Type="Edm.String" MaxLength="255"/>
                <ReturnType Type="AIService.return_AIService_askQuestion"/>
            </Action>
            <Action Name="healthCheck" IsBound="false">
                <ReturnType Type="AIService.return_AIService_healthCheck"/>
            </Action>
            <Action Name="uploadPDF" IsBound="false">
                <ReturnType Type="AIService.return_AIService_uploadPDF"/>
            </Action>
            <Action Name="processPDF" IsBound="false">
                <Parameter Name="fileName" Type="Edm.String" MaxLength="255"/>
                <ReturnType Type="AIService.return_AIService_processPDF"/>
            </Action>
            <Action Name="askAboutPDF" IsBound="false">
                <Parameter Name="question" Type="Edm.String" MaxLength="1000"/>
                <Parameter Name="fileName" Type="Edm.String" MaxLength="255"/>
                <Parameter Name="selectedModel" Type="Edm.String" MaxLength="255"/>
                <ReturnType Type="AIService.return_AIService_askAboutPDF"/>
            </Action>
            <Action Name="listUploadedPDFs" IsBound="false">
                <ReturnType Type="Collection(AIService.return_AIService_listUploadedPDFs)" Nullable="true"/>
            </Action>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>